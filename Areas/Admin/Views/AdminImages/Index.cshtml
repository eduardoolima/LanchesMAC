<table border="0" width="100%">
    <tr>
        <td>
            <form method="post" enctype="multipart/form-data" asp-controller="AdminImages" asp-action="UploadFiles">
                <img src="~/images/icons8-fazer-upload.gif" width="200" height="150" />

                <div class="form-group">
                    <div class="col-md-10">
                        <p><b>Selecione um ou mais arquivos para enviar:</b></p>
                        <p>(Arquivos com o mesmo nome serão sobrescritos)</p>
                        <input type="file" name="files" multiple class="button btn-outline-primary"/>
                    </div>
                </div>
                <hr />
                <div class="form-group">
                    <div class="col-md-10">
                        <input type="submit" value="Enviar Arquivo(s)" class="button btn-outline-primary" />
                    </div>
                </div>
            </form>
        </td>
        <td>
            <form method="post" enctype="multipart/form-data" asp-controller="AdminImages" asp-action="GetImages">
                <img src="data:image/png;base64,">

                <div class="form-group">
                    <div class="col-md-10">
                        <p><strong>Vizualizar imagens ma pasta do servidor</strong></p>                                                
                    </div>
                    <hr />
                    <div class="form-group">
                        <div class="col-md-10">
                            <input type="submit" value="Visualizar Arquivos no servidor" class="button btn-outline-primary" />
                        </div>
                    </div>
                </div>
            </form>
        </td>
    </tr>
</table>
<div>
    @if(ViewData["Deleted"] != null){
        <li>@ViewData["Deleted"]</li>
    }
</div>